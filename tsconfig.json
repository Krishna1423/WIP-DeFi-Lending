{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext", "ES2020"],
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
